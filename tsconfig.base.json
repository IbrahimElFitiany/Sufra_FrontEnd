{
  "compilerOptions": {
    "target": "esnext",                  
    "module": "esnext",

    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "moduleResolution": "bundler",

    "jsx": "react-jsx",
    "esModuleInterop": true,                  
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true
  }
}